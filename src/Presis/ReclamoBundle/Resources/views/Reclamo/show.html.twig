{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Reclamo</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Tipo</th>
                <td>{{ entity.tipo }}</td>
            </tr>
            <tr>
                <th>Pendiente</th>
                <td>{{ entity.pendiente }}</td>
            </tr>
            <tr>
                <th>Nro</th>
                <td>{{ entity.nro }}</td>
            </tr>
            <tr>
                <th>Fecha</th>
                <td>{{ entity.fecha|date('d/m/Y H:i:s') }}</td>
            </tr>
            <tr>
                <th>Fechalimite</th>
                <td>{{ entity.fechaLimite|date('d/m/Y H:i:s') }}</td>
            </tr>
            <tr>
                <th>Fecharesuelto</th>
                <td>{{ entity.fechaResuelto|date('d/m/Y H:i:s') }}</td>
            </tr>
            <tr>
                <th>Resolvio</th>
                <td>{{ entity.resolvio }}</td>
            </tr>
            <tr>
                <th>Direccion</th>
                <td>{{ entity.direccion }}</td>
            </tr>
            <tr>
                <th>Telefono</th>
                <td>{{ entity.telefono }}</td>
            </tr>
            <tr>
                <th>Detalle</th>
                <td>{{ entity.detalle }}</td>
            </tr>
            <tr>
                <th>Observaciones</th>
                <td>{{ entity.observaciones }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('reclamo') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('reclamo_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
