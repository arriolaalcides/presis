{% extends '::base.html.twig' %}
{% block banner %}
{% endblock %}

{% block contenido %}
    <div class="container container-round col-md-offset-1 col-md-4">

        {#{{ form_start(delete_form, { 'style': 'horizontal' }) }}#}
        <fieldset>
            <legend>Seguimiento</legend>
            {{ form_control_static('Id',entity.id) }}
            {{ form_control_static('Receptor Nombre',entity.receptorNombre) }}
            {{ form_control_static('Receptor Apellido',entity.receptorApellido) }}
            {{ form_control_static('Receptor Fecha Hora',entity.receptorFechaHora|date('d/m/Y')) }}
            {{ form_control_static('Timestamp Modificacion',entity.timestampModificacion|date('d/m/Y H:i:s')) }}
            <br>
            <div class="form-group">	
            <br>
                <!--<label class="col-md-4 control-label" for="button1id"></label>-->
                <div class="col-md-2">
                <div class="clearboth">&nbsp;</div>
                    <a href="{{ path('tracker_movistar_editar') }}" class="btn btn-primary">Volver</a>
                        {#{{ form_widget(delete_form.submit) }}#}
                </div>
            </div>
            <div class="clearboth">&nbsp;</div>
        </fieldset>
        {#{{ form_end(delete_form) }}#}


        {% include 'BraincraftedBootstrapBundle::flash.html.twig' %}
    </div>
    <div class="clearboth">&nbsp;</div>
{% endblock %}
