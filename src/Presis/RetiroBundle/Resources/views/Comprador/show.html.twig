{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Comprador</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Calle</th>
                <td>{{ entity.calle }}</td>
            </tr>
            <tr>
                <th>Apenom</th>
                <td>{{ entity.apenom }}</td>
            </tr>
            <tr>
                <th>Altura</th>
                <td>{{ entity.altura }}</td>
            </tr>
            <tr>
                <th>Piso</th>
                <td>{{ entity.piso }}</td>
            </tr>
            <tr>
                <th>Dpto</th>
                <td>{{ entity.dpto }}</td>
            </tr>
            <tr>
                <th>Otherinfo</th>
                <td>{{ entity.otherInfo }}</td>
            </tr>
            <tr>
                <th>Cp</th>
                <td>{{ entity.cp }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('comprador') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('comprador_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
