<pdf>
    <dynamic-page font-type="DejaVuSans">
        <div id="contenedor" width="100%" height="100%" border.color="black"><!--contenedor-->
            <div id="fila1" width="100%" height="140px" border.color="black"><!--fila1-->
                <div id="col1" width="60%" height="140px" border.color="black" float="left"><!--col1-->
                    <div id="sub-col1" width="100%" height="70px" border.color="black">
                        <img src="{{ pdf_image('PresisRetiroBundle:logo_moto_norte.png') }}" width="250px" height="69px" />
                    </div>
                    <div id="sub-col2" width="100%" height="60px" border.color="black" padding-top="10px">
                        <b>{FRAGILIDAD}</b>
                    </div>
                </div><!--col1-->
                <div id="col2" width="40%" height="140px" border.color="black" float="left" text-align="right"><!--col2-->
                    <div id="fila-40" width="100%" height="25px" padding-bottom="3px" padding-top="3px">
                        {{ retiro.servicio.descripcion }}
                    </div>
                    <div extends="fila-40">
                        {PRESTADOR}
                    </div>
                    <div extends="fila-40">
                        {RANGO}
                    </div>
                    <div extends="fila-40">
                       {{ retiro.fechHora|date("d/m/Y H:i") }}
                    </div>
                </div><!--col2-->
            </div><!--fila1-->
            <div id="fila2" width="100%" height="180px" border.color="black"><!--fila2-->
                <div id="fila2-col1" width="60%" height="180px" border.color="black" float="left"><!--fila2-col1-->
                    <div id="titulo" width="60%" border.color="black" padding-left="5px">
                        <h3><b>DESTINATARIO</b></h3>
                    </div>
                    <div id="fila60" width="100%" height="25px" padding-left="5px" padding-top="5px">
                        {{ retiro.comprador.calle ~ ' ' ~ retiro.comprador.altura ~ ' ' ~ retiro.comprador.piso ~ ' ' ~ retiro.comprador.dpto}}
                    </div>
                    <div extends="fila60">
                        {{ retiro.comprador.apenom }}
                    </div>
                    <div extends="fila60">
                        {{ retiro.comprador.otherInfo }}
                    </div>
                </div><!--fila2-col1-->
                <div id="fila2-col2" width="40%" height="180px" border.color="black" float="left" text-align="right"><!--fila2-col2-->
                    <div extends="fila-40">
                        {PROVINCIA}
                    </div>
                    <div extends="fila-40">
                        {SUB ZONA}
                    </div>
                    <div extends="fila-40">
                        {CORDON}
                    </div>
                </div><!--fila2-col2-->
            </div><!--fila2-->
            <div extends="fila1"><!--fila1-->
                <div id="tracking" width="35%" border.color="black" padding-left="5px">
                    <h3><b>TRACKING</b></h3>
                </div>
                <div id="barcode" width="80%" font-size="36px">
                    <barcode type="code128" code="{{ retiro.id }}" margin-left="130px" margin-top="20px" float="right" bar-height="100px" bar-thin-width="6px" bar-thick-width="6px" />
                </div>
            </div><!--fila1-->
            <div extends="fila2"><!--fila1-->
                <div extends="tracking">
                    <h3><b>SUCURSAL RETIRO</b></h3>
                </div>
                <div id="fila50" width="50%" float="left" padding-top="5px">
                    {{ retiro.sender.calle ~ ' ' ~ retiro.sender.altura ~ ' ' ~ retiro.sender.piso ~ ' ' ~ retiro.sender.dpto}}
                </div>
                <div extends="fila50">
                    {{retiro.sucursal.descripcion}}
                </div>
                <div id="fila100">
                    {{retiro.cliente.empresa}}
                </div>
            </div><!--fila1-->
            <div id="a-definir" text-align="center">
                *ESPACIO A DEFINIR*
            </div>
        </div><!--contenedor-->
    </dynamic-page>
</pdf>