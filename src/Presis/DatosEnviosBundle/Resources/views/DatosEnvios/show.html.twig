    <script type="text/javascript">

        var $table = $("#retiros");

        var $pagina = 0;
        var $limite = 10;

        $(function () {
            buildTable($table, 20, 20);
            $('#fixedNumber').change(function () {
                buildTable($table, 20, 20);
            });
        });

        function buildTable($el, cells, rows) {
            $el.bootstrapTable('destroy').bootstrapTable({
                toolbar: '.toolbar'
            });
        }

        function acciones(value, row){
            var botones = "<a href="+Routing.generate('retiro_showguia', {id: row.id} )+" target='_blank'><i class='fa fa-file-pdf-o fa-2x'></i></a>" +
            "<a href="+Routing.generate('retiro_showetiqueta',{id: row.id} )+" target='_blank'><i class='fa fa-print fa-2x'></i></a>";
            {% if is_granted('ROLE_ADMIN') or is_granted('ROLE_SUCURSAL') %}
             botones = "<a href="+Routing.generate('datosenvios_edit',{id: row.id} )+" target='_blank'><i class='fa fa-search fa-2x'></i></a>" +
             "<a href="+Routing.generate('retiro_showetiqueta',{id: row.id} )+" target='_blank'><i class='fa fa-print fa-2x'></i></a>" +
             botones;
            {% endif %}
             return botones;
        }

        function formatLink(val,row){
            val = "<a href="+$("#tracker").val()+"?tracking="+val+" target='_blank' class='link'>"+val+"</a>";
            return val;
        }



        $(function () {
            $('#toolbar').find('select').change(function () {
                $table.bootstrapTable('destroy').bootstrapTable({
                    exportDataType: $(this).val()
                });
            });
        })

        $(document).ready(function () {

            if ($("select[id*='retiro_distribuidor'] option:selected").val()) {
                readonly("select[id*='retiro_distribuidor']", true);
            }

            //var $table = $('#retiros');
            $table.bootstrapTable()
                    .on('page-change.bs.table', function (event, page, limit) {
                        $pagina = page - 1;
                        $limite = limit;

                        buscarGuias();
                    });

           /* $('.ret').on('click','.link', function(event){
                event.preventDefault();
                alert(getIdSelection());
                $.blockUI({message: "<h3>Buscando datos...</h3>"});
                $.ajax({
                    dataType: "json",
                    url: Routing.generate("datosenvios_find_tracker"),
                    type: 'GET',
                    data: {
                        "id": function (d) {
                            return getIdSelection();
                        }
                    },
                    success: function (data) {
                        var $tableTracker = $('#trackeo');
                        $tableTracker.bootstrapTable()
                        $tableTracker.bootstrapTable('load', data);
                        $("#modalTable").modal('show');
                    },
                    error: function (error) {
                        BootstrapDialog.show({
                            title: 'Error',
                            message: error.responseText
                        });
                    }
                });
                $.unblockUI();
            });*/

            function getIdSelection() {
                return $.map($table.bootstrapTable('getSelections'), function (row) {
                    return row.id;
                });
            }

            $('#presis_retirobundle_retiro_sinEstado').change(function() {
                var deshabilitarEstados = $(this).is(":checked");
                var $estado = $('#presis_retirobundle_retiro_estado');
                if(deshabilitarEstados)
                        $estado.val([]);
                $estado.prop('disabled', deshabilitarEstados);
            });


            $("#find").click(function(){
                $("#demo").collapse('hide');
                $("#boton").text(function(i,old){
                    return old=='ABRIR' ?  'CERRAR' : 'ABRIR';
                });
            });


            $("#exportar").click(function(){
                $limite = 0;

                exportarGuias();

                $limite = 10;
            });

            var obtenerParametrosBusqueda = function() {
                var $idDesde = $("#presis_retirobundle_retiro_id").val();
                var $idHasta = $("#presis_retirobundle_retiro_idHasta").val();

                var $remito = $("#presis_retirobundle_retiro_remito").val();
                var $fechaDesde = $("#presis_retirobundle_retiro_datosenvios_fecha").val();
                var $fechaHasta = $("#presis_retirobundle_retiro_datosenvios_hasta").val();
                var $cliente = $("#presis_retirobundle_retiro_datosenvios_cliente").val();
                var $nroCta = $("#presis_retirobundle_retiro_datosenvios_nroCta").val();
                var $remitente = $("#presis_retirobundle_retiro_datosenvios_remitente").val();
                var $destinatario = $("#presis_retirobundle_retiro_datosenvios_destinatario").val();
                var $planillaDesde = $("#presis_retirobundle_retiro_planillaDesde").val();
                var $planillaHasta = $("#presis_retirobundle_retiro_planillaHasta").val();
                var $sucursal = $("#presis_retirobundle_retiro_sucursal").val();
                var $ultimoEstadoDesde = $("#presis_retirobundle_retiro_ultimoEstadoDesde").val();
                var $ultimoEstadoHasta = $("#presis_retirobundle_retiro_ultimoEstadoHasta").val();
                var $nroPlanilla = $("#presis_retirobundle_retiro_nroPlanilla").val();
                var $distribuidor = $("#presis_retirobundle_retiro_distribuidor option:selected").text();
                var $estado = $("#presis_retirobundle_retiro_estado").val();
                var $guiaAgente = $("#presis_retirobundle_retiro_datosenvios_guiaAgente").val();
                var $zona = $("#presis_retirobundle_retiro_zona option:selected" ).text();
                var $facturado = $("#presis_retirobundle_retiro_datosenvios_facturado").val();
                var $fechaUdesde = $("#presis_retirobundle_retiro_fechaUdesde").val();
                var $fechaUhasta = $("#presis_retirobundle_retiro_fechaUhasta").val();
                var $conContrareembolso = $("#presis_retirobundle_retiro_datosenvios_contrareembolso").val();

                var $cordonOrigen = $("#presis_retirobundle_retiro_datosenvios_cordonOrigen option:selected" ).text();
                var $cordonDestino = $("#presis_retirobundle_retiro_datosenvios_cordonDestino option:selected" ).text();

                var $fechaUplanillaDesde = $("#presis_retirobundle_retiro_fechaUplanillaDesde").val();
                var $fechaUplanillaHasta = $("#presis_retirobundle_retiro_fechaUplanillaHasta").val();

                var $fechaFacturaDesde = $("#presis_retirobundle_retiro_fechaFacturaDesde").val();
                var $fechaFacturaHasta = $("#presis_retirobundle_retiro_fechaFacturaHasta").val();

                var $sinEstado = $("#presis_retirobundle_retiro_sinEstado").is(":checked");

                var $sinFactura = $("#presis_retirobundle_retiro_sinFactura").is(":checked");

                if($.trim($idDesde)!='' && $.trim($idHasta)==''){
                    $("#presis_retirobundle_retiro_idHasta").val($idDesde);
