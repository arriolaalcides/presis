{% extends '::base.html.twig' %}
{% block contenido %}
    <div class="panel panel-default">
        <div class="panel-heading">
            <h3 class="panel-title pull-left">
                Busqueda de guias
            </h3>
            <button class="btn btn-default pull-right btn-sm" data-toggle="collapse" data-target="#demo" id="boton"></button>
            <div class="clearfix"></div>
        </div>
        <input type="hidden" name="url" id="url" value="{{ url }}" />
        <input type="hidden" name="tracker" id="tracker" value="{{ tracker }}" />
        <div class="panel-body collapse in" id="demo">
            {{ form_start(form) }}
                <div class="row">
                    <div class="col-lg-9">
                        <div class="row">
                            <div class="form-group">
                                {% if not sucursal %}
                                    <div class="col-lg-3">
                                        {{ form_row(form.sucursal, {'label_col':1,'widget_col': 12, attr:{class: 'input-sm', onkeypress:'return tabular(event,this)'} }) }}
                                    </div>
                                {% else %}
                                    <div class="col-lg-3">
                                        {{ form_row(form.sucursal, {value: "" ~ sucursal.id, 'label_col':1,'widget_col': 12, attr:{class: 'input-sm', onkeypress:'return tabular(event,this)',  'disabled': 'true'} }) }}
                                    </div>
                                {% endif %}
                            </div>
                        </div>
                        <div class="row">
                            <div class="form-group">
                                <div class="col-lg-2">
                                    {{ form_row(form.datosenvios.fecha, {'label_col':1,'widget_col': 2, attr:{class: 'input-sm date-picker', onkeypress:'return tabular(event,this)'} }) }}
                                </div>
                                <div class="col-lg-2">
                                    {{ form_row(form.datosenvios.hasta, {'label_col':1,'widget_col': 2, attr:{class: 'input-sm date-picker', onkeypress:'return tabular(event,this)'} }) }}
                                </div>
                                <div class="col-lg-2">
                                    {{ form_row(form.planillaDesde, {'label_col':1,'widget_col': 2, attr:{class: 'input-sm', onkeypress:'return tabular(event,this)'} }) }}
                                </div>
                                <div class="col-lg-2">
                                    {{ form_row(form.planillaHasta, {'label_col':1,'widget_col': 2, attr:{class: 'input-sm', onkeypress:'return tabular(event,this)'} }) }}
                                </div>
                                <div class="col-lg-2">
                                    {{ form_row(form.fechaUdesde, {'label_col':1,'widget_col': 2, attr:{class: 'input-sm date-picker', onkeypress:'return tabular(event,this)'} }) }}
                                </div>
                                <div class="col-lg-2">
                                    {{ form_row(form.fechaUhasta, {'label_col':1,'widget_col': 2, attr:{class: 'input-sm date-picker', onkeypress:'return tabular(event,this)'} }) }}
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="form-group">
                                <div class="col-lg-2">
                                    {{ form_row(form.fechaUplanillaDesde, {'label_col':1,'widget_col': 2, attr:{class: 'input-sm date-picker', onkeypress:'return tabular(event,this)'} }) }}
                                </div>
                                <div class="col-lg-2">
                                    {{ form_row(form.fechaUplanillaHasta, {'label_col':1,'widget_col': 2, attr:{class: 'input-sm date-picker', onkeypress:'return tabular(event,this)'} }) }}
                                </div>
                                <div class="col-lg-2">
                                    {{ form_row(form.fechaFacturaDesde, {'label_col':1,'widget_col': 2, attr:{class: 'input-sm date-picker', onkeypress:'return tabular(event,this)'} }) }}
                                </div>
                                <div class="col-lg-2">
                                    {{ form_row(form.fechaFacturaHasta, {'label_col':1,'widget_col': 2, attr:{class: 'input-sm date-picker', onkeypress:'return tabular(event,this)'} }) }}
                                </div>
                                <div class="col-lg-2">
                                    {{ form_row(form.id, {'label_col': 1, 'widget_col': 2, attr:{class: 'input-sm', onkeypress:'return tabular(event,this)'} }) }}
                                </div>
                                <div class="col-lg-2">
                                    {{ form_row(form.idHasta, {'label_col': 1, 'widget_col': 2, attr:{class: 'input-sm', onkeypress:'return tabular(event,this)'} }) }}
                                </div>
                            </div>

                            <div class="form-group">
                                <div class="col-lg-1">
                                    {{ form_row(form.datosenvios.cordonOrigen, {'label_col': 1, 'widget_col': 2, attr:{class: 'input-sm', onkeypress:'return tabular(event,this)'} }) }}
                                </div>
                                <div class="col-lg-1">
                                    {{ form_row(form.datosenvios.cordonDestino, {'label_col': 1, 'widget_col': 2, attr:{class: 'input-sm', onkeypress:'return tabular(event,this)'} }) }}
                                </div>
                                <div class="col-lg-2">
                                    {{ form_row(form.zona, {'label_col':1,'widget_col': 2, attr:{class: 'input-sm', onkeypress:'return tabular(event,this)'} }) }}
                                </div>
                                <div class="col-lg-2">
                                    {{ form_row(form.remito, {'label_col':1,'widget_col': 2, attr:{class: 'input-sm', onkeypress:'return tabular(event,this)'} }) }}
                                </div>
                                <div class="col-lg-2">
                                    {{ form_row(form.datosenvios.guiaAgente, {'label_col': 1, 'widget_col': 2, attr:{class: 'input-sm', onkeypress:'return tabular(event,this)'} }) }}
                                </div>
                                <div class="col-lg-2">
                                    {{ form_row(form.datosenvios.cliente, {'label_col':1,'widget_col': 2, attr:{class: 'input-sm', onkeypress:'return tabular(event,this)'} }) }}
                                </div>
                                <div class="col-lg-2">
                                    {{ form_row(form.datosenvios.nroCta, {'label_col':1,'widget_col': 2, attr:{class: 'input-sm', onkeypress:'return tabular(event,this)'} }) }}
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="form-group">
                                <div class="col-lg-2">
                                    {{ form_row(form.datosenvios.facturado, {'label_col':1,'widget_col': 2, attr:{class: 'input-sm', onkeypress:'return tabular(event,this)'} }) }}
                                </div>
                                <div class="col-lg-2">
                                    {{ form_row(form.datosenvios.contrareembolso, {'label_col':1,'widget_col': 2, attr:{class: 'input-sm', onkeypress:'return tabular(event,this)'} }) }}
                                </div>
                                <div class="col-lg-2">
                                    {{ form_row(form.nroPlanilla, {'label_col':1,'widget_col': 2, attr:{class: 'input-sm', onkeypress:'return tabular(event,this)'} }) }}
                                </div>
                                {% if not distribuidor %}
                                    <div class="col-lg-2">
                                        {{ form_row(form.distribuidor, {'label_col':1,'widget_col': 2, attr:{class: 'input-sm', onkeypress:'return tabular(event,this)'} }) }}
                                    </div>
                                {% else %}
                                    <div class="col-lg-2">
                                        {{ form_row(form.distribuidor, {value: "" ~ distribuidor.id, 'label_col':1,'widget_col': 2, attr:{class: 'input-sm', onkeypress:'return tabular(event,this)'} }) }}
                                    </div>
                                {% endif %}
                                {% if is_granted('ROLE_ADMIN') %}
                                <div class="col-lg-2">
                                    <div class="checkbox">
                                        <label>
                                            {{ form_row(form.sinEstado, {'label_col':2,'widget_col': 2, attr:{class: 'input-sm', onkeypress:'return tabular(event,this)'} }) }}
                                        </label>
                                    </div>
                                </div>
                                {% endif %}
                            </div>
                        </div>
                        <div class="row">
                            <div class="form-group">
                                {% if is_granted('ROLE_CLIENTE')== true %}
                                    <div class="col-lg-2">
                                        {{ form_row(form.user, {'label_col':1,'widget_col': 2, attr:{class: 'input-sm', onkeypress:'return tabular(event,this)'} }) }}
                                    </div>
                                {% endif %}
                                {% if is_granted('ROLE_CLIENTE')==true %}
                                    <div class="col-lg-2">
                                        {{ form_row(form.datosenvios.remitente, {'label_col':1,'widget_col': 2, attr:{class: 'input-sm', onkeypress:'return tabular(event,this)'} }) }}
                                    </div>
                                    <div class="col-lg-2">
                                        {{ form_row(form.datosenvios.destinatario, {'label_col':1,'widget_col': 2, attr:{class: 'input-sm', onkeypress:'return tabular(event,this)'} }) }}
                                    </div>
                                {% endif %}
                            </div>
                        </div><!-- content-row -->
                    </div>
                    <div class="col-lg-3">
                        <div class="form-group">
                            <div class="col-lg-12">
                                {{ form_row(form.estado, {'label_col':1,'widget_col': 2, attr:{onkeypress:'return tabular(event,this)'} }) }}
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="form-group">
                        <div class="col-lg-7 col-lg-offset-2">
                            <button type="button" class="btn btn-success btn-sm" id="find">BUSCAR GUIAS</button>
                            <!--<button type="button" class="btn btn-success btn-sm" id="exportar">EXPORTAR</button>-->
                            <button type="button" class="btn btn-danger btn-sm" id="prints">IMPRIMIR SELECCIONADOS</button>
                            <button type="button" class="btn btn-warning btn-sm" id="etiquetas">IMPRIMIR ETIQUETAS</button>
                            {% if is_granted('ROLE_CLIENTE')==false %}
                                <button type="button" class="btn btn-warning btn-sm" id="archivo">GENERAR ARCHIVO</button>
                            {% endif %}
                            {% if is_granted('ROLE_CLIENTE') %}
                                <button type="button" class="btn btn-warning btn-sm" id="archivo2">GENERAR ARCHIVO</button>
                            {% endif %}
                        </div>
                    </div>
                </div>
            {{ form_end(form) }}
            <br>
        </div>
    </div>
